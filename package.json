{
  "name": "@alesmenzel/number-format",
  "version": "3.1.2",
  "description": "Number formatting utility",
  "keywords": [
    "number",
    "formatting",
    "utility"
  ],
  "contributors": [
    "Ales Menzel <alesmenzel@gmail.com> (https://github.com/alesmenzel)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alesmenzel/number-format.git"
  },
  "bugs": {
    "url": "https://github.com/alesmenzel/number-format/issues"
  },
  "license": "MIT",
  "main": "./dist/number-format.js",
  "module": "./number-format.js",
  "scripts": {
    "build": "run-s clean build:*",
    "build:umd": "webpack",
    "clean": "rimraf ./dist",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest --coverage .",
    "format": "prettier --write \"./**/*.{js,json,css,md}\"",
    "prepublishOnly": "run-s lint test build",
    "publish:latest": "npm publish --access public --tag latest",
    "publish:next": "npm publish --access public --tag next"
  },
  "dependencies": {
    "is-number": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "run-s lint test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
