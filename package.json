{
  "name": "@alesmenzel/number-format",
  "version": "2.0.0-rc.1",
  "description": "Number formatting utility",
  "keywords": [
    "number",
    "formatting",
    "utility"
  ],
  "contributors": [
    "Ales Menzel <alesmenzel@gmail.com> (https://github.com/alesmenzel)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alesmenzel/number-format.git"
  },
  "bugs": {
    "url": "https://github.com/alesmenzel/number-format/issues"
  },
  "license": "MIT",
  "main": "./dist/number-format.js",
  "module": "./number-format.js",
  "scripts": {
    "build": "run-s clean build:*",
    "build:umd": "webpack",
    "clean": "rimraf ./dist",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest --coverage .",
    "format": "prettier --write \"./**/*.{js,json,css,md}\"",
    "prepublishOnly": "run-s lint test build",
    "publish:latest": "npm publish --access public --tag latest",
    "publish:next": "npm publish --access public --tag next"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.16.4",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "run-s lint test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "src",
    "number-format.js",
    "dist",
    "LICENSE",
    "README.md"
  ]
}
